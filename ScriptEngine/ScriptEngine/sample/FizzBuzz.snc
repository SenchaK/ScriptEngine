class Vector2 {
	$x;
	$y;
	$z;
	function init( $_x , $_y , $_z ){
		%this.x = $_x;
		%this.y = $_y;
		%this.z = $_z;
	}
}


function main(){
	$pos as Vector2[10];
	for( $i = 0 ; $i < 10 ; $i += 1 ){
		$pos[$i].init( 50 * $i , 25 * $i , 70 * $i );
	}
	for( $i = 0 ; $i < 10 ; $i += 1 ){
		Log( ToString( $pos[$i].x ) + "," + ToString( $pos[$i].y ) + "," + ToString( $pos[$i].z ) );
	}
}


