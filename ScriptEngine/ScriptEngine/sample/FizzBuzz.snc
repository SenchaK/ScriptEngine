class Vector2 {
	$x;
	$y;
	
	function init( $_x , $_y ){
		%this.x = $_x;
		%this.y = $_y;
	}
}


function main(){
	$value as Vector2;
	$value.init( 320 , 240 );
	
	for( $i = 0 ; $i < 100 ; $i+=1 ){
		$value.x += 5;
		$value.y += 10;
		Log( "("+ToString($value.x)+","+ToString($value.y)+")" );
	}
}

