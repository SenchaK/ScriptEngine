struct VEC2{
	$x;
	$y;
	function hoge(){
		Log( "Hello World!!" );
		%this.x = 320;
		%this.y = %this.y * 240;
	}

	function init( $_x , $_y ){
		%this.x = $_x;
		%this.y = $_y;
		%this.hoge();
	}
}


function main(){
	$value as VEC2;
	$value.x = 0;
	$value.y = 0;
	$value.init( 0 , 3 );
	Log( ToString( $value.x ) );
	Log( ToString( $value.y ) );
}

