class FizzBuzz {
	$value;
	
	function fizzbuzz( $start , $end ){
		%this.value = $start;
		while( %this.value < $end ){
			if( %this.value % 3 == 0 && %this.value % 5 == 0 ){
				Log( "FizzBuzz" );
			}
			else if( %this.value % 3 == 0 ){
				Log( "Fizz" );
			}
			else if( %this.value % 5 == 0 ){
				Log( "Buzz" );
			}
			else{
				Log( ToString( %this.value ) );
			}
			%this.value += 1;
		}
	}
}


function main(){
	$fb as FizzBuzz;
	$fb.fizzbuzz( 1 , 101 );
}

